{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowOnlyExplicableApiActions",
      "Effect": "Allow",
      "Action": [
        "dynamodb:Scan",
        "dynamodb:GetItem",
        "dynamodb:Query",
        "dynamodb:PutItem",
        "dynamodb:UpdateItem",
        "dynamodb:DeleteItem",
        "dynamodb:ListTables",
        "es:ESHttpGet",
        "es:ESHttpPost",
        "es:ESHttpPut",
        "es:ESHttpDelete",
        "es:ListDomainNames"
      ],
      "Resource": "*"
    }
  ]
}
